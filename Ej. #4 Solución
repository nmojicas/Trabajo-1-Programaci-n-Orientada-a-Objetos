# EJERCICIO RESUELTO 4 
public class Main {
    public static void main(String[] args) {
        int edadJuan = 9;
        int edadAlberto = (2 * edadJuan) / 3;
        int edadAna = (4 * edadJuan) / 3;
        int edadMama = edadAlberto + edadJuan + edadAna;
        System.out.println("Juan tiene " + edadJuan + " a単os");
        System.out.println("Alberto tiene " + edadAlberto + " a単os");
        System.out.println("Ana tiene  " + edadAna + " a単os");
        System.out.println("La madre entonces tiene " + edadMama + " a単os");
    }
}
